
<div class="container">
    <div class="row justify-content-center mt-5">
        <div class="col-12 text-center">
            <h1>Lista de productos</h1>
        </div>
    </div>
    <div class="row">
        {{#each productos}}
            <div class="col-sm-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">{{this.tittle}} </h5>
                        <p class="card-text">{{this.description}}</p>
                        <p class="card-text">{{this.price}}</p>
                    </div>
                </div>
            </div>
    {{/each}}
    </div>
    
</div>
